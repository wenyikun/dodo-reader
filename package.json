{
  "name": "dodo-reader",
  "displayName": "Dodo Reader",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dodo-reader.openEpub",
        "title": "Open ePub File"
      }
    ],
    "customEditors": [
      {
        "viewType": "dodo-reader.epubEditor",
        "displayName": "EPUB Editor",
        "selector": [
          {
            "filenamePattern": "*.epub"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run web:build && npm run compile",
    "compile": "tsup src/extension.ts --dts --minify --external=vscode",
    "watch": "npm run compile -- --watch",
    "web:build": "vite build",
    "web:watch": "vite build --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}